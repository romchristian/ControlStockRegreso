<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity$PlaceholderFragment"
    android:gravity="center_vertical|center_horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/textView_fecha"
        android:layout_alignRight="@+id/autoCompleteTextView"
        android:layout_alignEnd="@+id/autoCompleteTextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/textView_resposable"
        android:layout_below="@+id/textView_fecha"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <AutoCompleteTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New AutoCompleteTextView"
        android:id="@+id/autoCompleteTextView"
        android:layout_below="@+id/textView_resposable"
        android:layout_alignLeft="@+id/textView_resposable"
        android:layout_alignStart="@+id/textView_resposable"
        android:layout_marginTop="33dp" />

    <AutoCompleteTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New AutoCompleteTextView"
        android:id="@+id/autoCompleteTextView2"
        android:layout_below="@+id/autoCompleteTextView"
        android:layout_alignLeft="@+id/autoCompleteTextView"
        android:layout_alignStart="@+id/autoCompleteTextView" />

    <AutoCompleteTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New AutoCompleteTextView"
        android:id="@+id/autoCompleteTextView3"
        android:layout_below="@+id/autoCompleteTextView2"
        android:layout_alignLeft="@+id/autoCompleteTextView2"
        android:layout_alignStart="@+id/autoCompleteTextView2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/autoCompleteTextView3"
        android:layout_alignLeft="@+id/autoCompleteTextView3"
        android:layout_alignStart="@+id/autoCompleteTextView3"
        android:layout_alignRight="@+id/autoCompleteTextView3"
        android:layout_alignEnd="@+id/autoCompleteTextView3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button"
        android:layout_below="@+id/editText"
        android:layout_alignLeft="@+id/editText"
        android:layout_alignStart="@+id/editText"
        android:layout_marginTop="32dp"
        android:layout_alignRight="@+id/editText"
        android:layout_alignEnd="@+id/editText" />



</RelativeLayout>
